<%- include("partials/header"); -%>



<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="">
    <label>Title</label>
    <input type="text" name="blog" class="form-control">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea name="blogBody" rows="6" cols="152" class="form-control"></textarea>
